# Setup plots for IEEE papers

# Set color cycle
# Set line style cycle as well (for black and white graphs)
axes.prop_cycle : (cycler('color', ['k', 'r', 'b', 'g']) + cycler('ls', ['-', '--', ':', '-.']))

# Set default figure size
figure.figsize : 3.3, 2.5
figure.dpi : 600

# Font sizes
font.size : 8
